<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test">  
  <select id="getItem">
    select *
    from testTable
    where id = #{id}
  </select>
  <update id="updateTestUser">
    update testTable
    set name = #{name}
    where id = #{id}
  </update>
  
</mapper>